{
    "version": 2,
    "builds": [
        { 
            "src": "index.js", 
            "use": "@now/node" 
        }
    ],
    "routes": [
    {
      "src": ".*",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Access-Control-Allow-Origin, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      },
      "continue": true
    }
  ]
}